<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/base_m_m">


    <RelativeLayout
        style="@style/base_m_w"
        android:layout_height="66dp"
        android:background="@color/white"
        android:paddingBottom="12dp"
        android:paddingLeft="@dimen/common_padding"
        android:paddingRight="@dimen/common_padding"
        android:paddingTop="12dp">

        <LinearLayout
            android:layout_centerVertical="true"
            style="@style/ll_w_w_v">
            <TextView
                android:id="@+id/tvTypePayLog"
                style="@style/text_14_black_1"
                android:text="充值" />

            <TextView
                android:id="@+id/tvTimePayLog"
                style="@style/text_12_black_3"
                android:layout_marginTop="6dp"
                android:layout_alignParentRight="true"
                android:text="2016-10-23 08:02" />

        </LinearLayout>

        <LinearLayout
            style="@style/ll_w_w_h"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true">

            <TextView
                android:id="@+id/tvNumPayLog"
                style="@style/text_16_green"
                android:text="+160" />

            <TextView
                style="@style/text_12_green"
                android:text="元" />
        </LinearLayout>
    </RelativeLayout>
</FrameLayout>